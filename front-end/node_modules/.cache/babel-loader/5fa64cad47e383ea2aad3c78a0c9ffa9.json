AAA,CAAA,IAAA,CAHA;;AAAA;AAIA,oBAAA,MAAA,CAAA,KAAA,EACA,OAAA,CACA,mGADA,CAAA;AAGA,gBAAA,MAAA,CAAA,IAAA,GAAA,SAAA;AARA;AAAA,uBASA,MAAA,CAAA,KAAA,CAAA,IAAA,CATA;;AAAA;AAUA,gBAAA,MAAA,CAAA,IAAA,GAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,qBAAA,EAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,cAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KA3DA;AA4DA,IAAA,OA5DA,mBA4DA,WA5DA,EA4DA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,qBAAA,EAAA,WAAA;AACA,KA9DA;AA+DA,IAAA,gBA/DA,8BA+DA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,qBAAA,EAAA,cAAA;AACA,KAjEA;AAkEA,IAAA,GAlEA,eAkEA,KAlEA,EAkEA;AACA,aAAA,KAAA,GAAA,CAAA;AACA,KApEA;AAqEA,IAAA,QArEA,oBAqEA,EArEA,EAqEA;AACA,WAAA,MAAA,GAAA;AACA,QAAA,KAAA,EAAA,uBADA;AAEA,QAAA,OAAA,EAAA,6BAAA,EAAA,CAAA,IAAA,GAAA,KAFA;AAGA,QAAA,UAAA,EAAA;AAHA,OAAA;AAKA,WAAA,YAAA,GAAA,UAAA;AACA,WAAA,SAAA,GAAA,EAAA,CAAA,EAAA;AACA,WAAA,gBAAA,GAAA,IAAA;AACA,KA9EA;AA+EA,IAAA,UA/EA,sBA+EA,EA/EA,EA+EA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,gBAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,KAAA,gBAAA,CAAA,CAAA,EAAA,EAAA,KAAA,EAAA,EAAA;AACA,eAAA,gBAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA;AACA;;AACA,WAAA,QAAA,CAAA,GAAA,CAAA,MAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,gBAAA,CAAA;AACA,WAAA,YAAA;AACA,KAvFA;AAwFA,IAAA,MAxFA,kBAwFA,KAxFA,EAwFA,OAxFA,EAwFA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,KAAA,EAAA,KAAA;AAAA,UAAA,IAAA,EAAA,OAAA;AAAA,UAAA,WAAA,EAAA;AAAA;AAFA,OAAA;AAIA,KA7FA;AA8FA,IAAA,WA9FA,yBA8FA;AACA,UAAA,KAAA,gBAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,YAAA,GAAA,WAAA;AACA,aAAA,MAAA,GAAA;AACA,UAAA,KAAA,EAAA,YADA;AAEA,UAAA,OAAA,EAAA,sDAFA;AAGA,UAAA,UAAA,EAAA;AAHA,SAAA;AAKA,aAAA,gBAAA,GAAA,IAAA;AACA;AACA,KAxGA;AAyGA,IAAA,WAzGA,yBAyGA;AACA,WAAA,gBAAA,GAAA,EAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,KAAA,gBAAA;AACA,WAAA,QAAA,CAAA,GAAA,CAAA,MAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,gBAAA,CAAA;AACA,WAAA,YAAA;AACA,KA9GA;AA+GA,IAAA,YA/GA,0BA+GA;AACA,WAAA,gBAAA,GAAA,KAAA;AACA;AAjHA,GAvBA;AA0IA,EAAA,OA1IA,qBA0IA;AACA,SAAA,gBAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,oBAAA,CAAA;AACA,SAAA,OAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,yBAAA,CAAA;AACA,SAAA,kBAAA,GACA,KAAA,MAAA,CAAA,OAAA,CAAA,8BAAA,CADA;AAEA,SAAA,IAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,qBAAA,CAAA;AACA,GAhJA;AAiJA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,aAAA,KAAA,gBAAA;AACA,KAHA;AAIA,IAAA,iBAJA,+BAIA;AACA,aAAA,KAAA,gBAAA,CAAA,MAAA,GAAA,CAAA;AACA,KANA;AAOA,IAAA,OAPA,qBAOA;AACA,aAAA,KAAA,IAAA;AACA,KATA;AAUA,IAAA,YAVA,0BAUA;AACA,aAAA,KAAA,IAAA,IAAA,SAAA,IAAA,KAAA,IAAA,IAAA,YAAA;AACA;AAZA;AAjJA,CAAA","sourcesContent":["<template>\r\n  <div id=\"main\">\r\n    <div id=\"header\">\r\n      <h1>Checking Smart Contracts</h1>\r\n    </div>\r\n    <div class=\"text\">\r\n      <span>DCR</span>\r\n    </div>\r\n    <div id=\"locate-1\">\r\n      <div class=\"text-1\">\r\n        <span>Blind Auction</span>\r\n      </div>\r\n      <div id=\"component\">\r\n        <div class=\"table table-striped table-hover\">\r\n          <table class=\"table\" border=\"1\">\r\n            <tr>\r\n              <th>#</th>\r\n              <th>LTL Property</th>\r\n              <th>Type</th>\r\n            </tr>\r\n            <tr v-for=\"data in datatable\" :key=\"data.id\">\r\n              <td>{{ data.id }}</td>\r\n              <td>{{ data.var }}</td>\r\n              <td><input type=\"checkbox\" /></td>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n      </div>\r\n      <div class=\"text-1\">\r\n        <span>EitherGame</span>\r\n      </div>\r\n      <div id=\"component\">\r\n        <div class=\"table table-striped table-hover\">\r\n          <table class=\"table\" border=\"1\">\r\n            <tr>\r\n              <th>#</th>\r\n              <th>LTL Property</th>\r\n              <th>Type</th>\r\n            </tr>\r\n            <tr v-for=\"data in datatable\" :key=\"data.id\">\r\n              <td>{{ data.id }}</td>\r\n              <td>{{ data.var }}</td>\r\n              <td><input type=\"checkbox\" /></td>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"text-2\">\r\n      <span>Free-content</span>\r\n    </div>\r\n    <div id=\"locate-1\">\r\n      <div class=\"text-1\">\r\n        <span>EitherLotto</span>\r\n      </div>\r\n      <div id=\"component\">\r\n        <div class=\"table table-striped table-hover\">\r\n          <table class=\"table\" border=\"1\">\r\n            <tr>\r\n              <th>#</th>\r\n              <th>LTL Property</th>\r\n              <th>Type</th>\r\n            </tr>\r\n            <tr v-for=\"data in datatable\" :key=\"data.id\">\r\n              <td>{{ data.id }}</td>\r\n              <td>{{ data.var }}</td>\r\n              <td><input type=\"checkbox\" /></td>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div id=\"processing-btn\">\r\n      <button\r\n        v-if=\"step == 'initial' || step == 'generating'\"\r\n        class=\"btn btn-primary-outline\"\r\n        @click=\"generate\"\r\n      >\r\n        Generate\r\n      </button>\r\n      <button v-else class=\"btn btn-primary-outline\" @click=\"check\">\r\n        Check\r\n      </button>\r\n    </div>\r\n    <div class=\"contain-process\">\r\n      <div id=\"processing-section\">\r\n        <div id=\"download\">\r\n          <a\r\n            v-if=\"showDownload\"\r\n            class=\"btn btn-primary btn-sm\"\r\n            href=\"\"\r\n            download=\"Solidity\"\r\n            >Download</a\r\n          >\r\n        </div>\r\n        <div id=\"initial\" v-if=\"step == 'initial'\"></div>\r\n        <div id=\"generating\" v-show=\"step == 'generating'\">\r\n          <div>The smart contract is generating...</div>\r\n          <div class=\"progress\" id=\"progress\">\r\n            <div\r\n              id=\"progress-bar-gen\"\r\n              class=\"progress-bar progress-bar-striped bg-warning\"\r\n              role=\"progressbar\"\r\n              style=\"width: 0%\"\r\n            ></div>\r\n          </div>\r\n        </div>\r\n        <div id=\"generated\" v-if=\"step == 'generated'\">\r\n          The generating process completed successfully\r\n        </div>\r\n        <div id=\"checking\" v-show=\"step == 'checking'\">\r\n          <div>The smart contract is checking...</div>\r\n          <div class=\"progress\" id=\"progress\">\r\n            <div\r\n              id=\"progress-bar-check\"\r\n              class=\"progress-bar progress-bar-striped bg-warning\"\r\n              role=\"progressbar\"\r\n              style=\"width: 0%\"\r\n            ></div>\r\n          </div>\r\n        </div>\r\n        <div id=\"checked\" v-if=\"step == 'checked'\">\r\n          The checking process completed successfully\r\n        </div>\r\n        <div id=\"results\" v-if=\"this.results.length > 0 && step == 'finish'\">\r\n          <div><h3>Results:</h3></div>\r\n          <div v-for=\"r in this.results\" :key=\"r\">{{ r }}</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CheckService from \"../services/check.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      datatable: [\r\n        { id: \"1\", var: \"GV1\" },\r\n        { id: \"2\", var: \"GV2\" },\r\n        { id: \"3\", var: \"GV3\" },\r\n        { id: \"4\", var: \"GV4\" },\r\n      ],\r\n      step: \"initial\",\r\n      list_selected_sc: [],\r\n      list_selected_vuls: [],\r\n      context: [],\r\n      user: { user_name: \"Dinh Thanh Ha\" },\r\n      error: true,\r\n      view: \"\",\r\n      results: [],\r\n      showConfirmation: false,\r\n      dialog: {},\r\n      confirmation: \"\",\r\n      currentSC: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async callToolHelena() {\r\n      const tName = \"helena\";\r\n      const res = await CheckService.callHelenaTools(tName);\r\n      if (res.status == 200 && res !== null && res != undefined) {\r\n        const mess = res.data.message;\r\n        this.results.push(mess);\r\n      }else{\r\n        this.results.push(\"Can't run HELENA tools\")\r\n      }\r\n    },\r\n    async callToolLTL() {\r\n      const tName = \"ltl\";\r\n      const res = await CheckService.callToolLTL(tName);\r\n      if (res.status == 200 && res !== null && res != undefined) {\r\n        const mess = res.data.message;\r\n        this.results.push(mess);\r\n      }else{\r\n        this.results.push(\"Can't run LTL tools\")\r\n      }\r\n    },\r\n    move(id) {\r\n      //let _this = this;\r\n      var elem = document.getElementById(id);\r\n      var width = elem.offsetWidth;\r\n      if (width >= 0) width = 0;\r\n      var inter = setInterval(() => {\r\n        if (width >= 100) {\r\n          clearInterval(inter);\r\n        } else {\r\n          width++;\r\n          elem.style.width = width + \"%\";\r\n        }\r\n      }, 10);\r\n    },\r\n    async delay(ms) {\r\n      return await new Promise((resolve) => setTimeout(resolve, ms));\r\n    },\r\n    async generate() {\r\n      this.step = \"generating\";\r\n      this.move(\"progress-bar-gen\");\r\n      await this.delay(2000);\r\n      this.step = \"generated\";\r\n      this.$store.commit(\"data/SetProcessView\", \"check-sc\");\r\n      this.callToolLTL();\r\n    },\r\n    async check() {\r\n      this.step = \"checking\";\r\n      this.move(\"progress-bar-check\");\r\n      await this.delay(2000);\r\n      if (this.error)\r\n        confirm(\r\n          \"We have discover some counter-examples with the smart contract code. Do you want to look at them?\"\r\n        );\r\n      this.step = \"checked\";\r\n      await this.delay(2000);\r\n      this.step = \"finish\";\r\n      this.$store.commit(\"data/SetProcessView\", \"finish\");\r\n      this.callToolHelena();\r\n    },\r\n    routing(processview) {\r\n      this.$store.commit(\"data/SetProcessView\", processview);\r\n    },\r\n    addSmartContract() {\r\n      this.$store.commit(\"data/SetProcessView\", \"sc-selection\");\r\n    },\r\n    inc(value) {\r\n      return value + 1;\r\n    },\r\n    removeSC(sc) {\r\n      this.dialog = {\r\n        title: \"Remove Smart Contract\",\r\n        message: \"Are you sure to remove '\" + sc.name + \"' ?\",\r\n        confirmbtn: \"Remove\",\r\n      };\r\n      this.confirmation = \"removeSC\";\r\n      this.currentSC = sc.id;\r\n      this.showConfirmation = true;\r\n    },\r\n    cfRemoveSC(id) {\r\n      for (var i = 0; i < this.list_selected_sc.length; i++) {\r\n        if (this.list_selected_sc[i].id === id) {\r\n          this.list_selected_sc.splice(i, 1);\r\n        }\r\n      }\r\n      this.$cookies.set(\"_ssc\", JSON.stringify(this.list_selected_sc));\r\n      this.closeConfirm();\r\n    },\r\n    editSC(sc_id, sc_name) {\r\n      this.$router.push({\r\n        name: \"EditSc\",\r\n        params: { sc_id: sc_id, name: sc_name, parent_path: \"/process\" },\r\n      });\r\n    },\r\n    removeAllSc() {\r\n      if (this.list_selected_sc.length > 0) {\r\n        this.confirmation = \"removeAll\";\r\n        this.dialog = {\r\n          title: \"Remove All\",\r\n          message: \"Are you sure to remove all selected smart contracts?\",\r\n          confirmbtn: \"Remove All\",\r\n        };\r\n        this.showConfirmation = true;\r\n      }\r\n    },\r\n    cfRemoveAll() {\r\n      this.list_selected_sc = [];\r\n      this.$store.commit(\"data/SetSelectedSC\", this.list_selected_sc);\r\n      this.$cookies.set(\"_ssc\", JSON.stringify(this.list_selected_sc));\r\n      this.closeConfirm();\r\n    },\r\n    closeConfirm() {\r\n      this.showConfirmation = false;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.list_selected_sc = this.$store.getters[\"data/GetSelectedSC\"];\r\n    this.context = this.$store.getters[\"data/GetSelectedContext\"];\r\n    this.list_selected_vuls =\r\n      this.$store.getters[\"data/GetSelectedVulnerbility\"];\r\n    this.view = this.$store.getters[\"data/GetProcessView\"];\r\n  },\r\n  computed: {\r\n    selectedSc() {\r\n      return this.list_selected_sc;\r\n    },\r\n    isSelectSomeThing() {\r\n      return this.list_selected_sc.length > 0;\r\n    },\r\n    getStep() {\r\n      return this.step;\r\n    },\r\n    showDownload() {\r\n      return this.step != \"initial\" && this.step != \"generating\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n#header {\r\n  text-align: center;\r\n  margin-top: 2%;\r\n  margin-bottom: 2%;\r\n  font-weight: bold;\r\n}\r\n.contain-process {\r\n  padding-bottom: 5%;\r\n}\r\n#locate-1 {\r\n  border: 1px solid;\r\n  width: 80%;\r\n  margin: 0 auto;\r\n  padding-bottom: 3%;\r\n}\r\n\r\n#processing-btn button {\r\n  cursor: pointer;\r\n  width: 15%;\r\n  height: 2%;\r\n  border: 1px solid #2196f3;\r\n  text-align: center;\r\n  color: #2196f3;\r\n  font-size: 13px;\r\n  line-height: 22px;\r\n  font-weight: 600;\r\n  padding: 4px 3px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n#locate-1 {\r\n  box-shadow: rgba(0, 0, 0, 0.05) 0px 6px 24px 0px,\r\n    rgba(0, 0, 0, 0.08) 0px 0px 0px 1px;\r\n  border: 1px solid #d9edf7;\r\n  border-radius: 10px;\r\n}\r\n\r\n#processing-btn button:hover {\r\n  background-color: #1079cf;\r\n  color: white;\r\n}\r\n.text-1 {\r\n  position: relative;\r\n  left: 15%;\r\n  top: 15px;\r\n  z-index: 1;\r\n  height: 30px;\r\n  width: 8%;\r\n  background: white;\r\n  text-align: center;\r\n}\r\n.text-2 {\r\n  position: relative;\r\n  left: 15%;\r\n  top: 15px;\r\n  z-index: 1;\r\n  height: 30px;\r\n  width: 6.6%;\r\n  background: white;\r\n  text-align: center;\r\n}\r\n.text {\r\n  position: relative;\r\n  left: 15%;\r\n  top: 15px;\r\n  z-index: 1;\r\n  height: 30px;\r\n  width: 3.6%;\r\n  background: white;\r\n  text-align: center;\r\n}\r\n#component {\r\n  margin: 0 auto;\r\n  width: 94%;\r\n  padding-top: 2%;\r\n  padding-bottom: 2%;\r\n  box-shadow: rgba(0, 0, 0, 0.05) 0px 6px 24px 0px,\r\n    rgba(0, 0, 0, 0.08) 0px 0px 0px 1px;\r\n  border: 1px solid #d9edf7;\r\n  border-radius: 10px;\r\n}\r\n.table {\r\n  width: 94%;\r\n  margin: 0 auto;\r\n}\r\n.table tr:first-child {\r\n  bac